/* thunar */
.thunar .standard-view .view { background-color: #181818; }/* main view */
.thunar grid paned grid  { background-color: #181818;}/* status bar */
.thunar toolbar, .thunar toolbar entry  { background-color: #181818; }/* toolbar */

.thunar .view { background-color: #181818;}
.thunar .view:selected { background-color: #8aa89c; color: #181818; }
.thunar > grid > toolbar * { -gtk-icon-transform: scale(0.6); }
.thunar { font-size: 17px }

