
/*  whisker menu  */
#whiskermenu-button
{
	color: #fff5dc;
	background-color: #285577;
	border-color: #457293;
	border-radius: 0px;
	margin-right: 3px;
}

/*  workspaces  */

#xfce-panel-toggle-button
{
	margin-right: 3px;
	background: none;
	background-color: #222222;
	border: 1px solid #313131;
	color: #fff5dc;
	border-radius: 0px;
	font-family: JetBrainsMono;
}

#xfce-panel-toggle-button:checked
{
	color: #fff5dc;
	border: 1px solid #457293;
	/* background: solid #285577; */
	background-color: #285577;
	font-family: JetBrainsMono;
}



/*  tabs  */
.xfce4-panel .tasklist .toggle:checked
{
	/* border: 1px solid #a1acb0; */
	/* border-bottom: 0px solid #a1acb0; */
	background: #285577;
	border: 1px solid #457293;
	border-radius: 0px;
	color: #fff5dc;
	font-family: JetBrainsMono;
}
.xfce4-panel .tasklist .toggle
{
	/* border: 1px solid #a1acb0; */
	background-color: #222222;
	border: solid 1px #313131;
	color: #fff5dc;
	border-bottom: 0px solid #a1acb0;
	border-radius: 0px;
	font-family: JetBrainsMono;
}

